#include<stdio.h>
int main()
{
    int a;
    printf("Enter the Number : ");
    scanf("%d",&a);
    for(int i=1;i<=100;i++)
    {
        if(a==(i*i))
         {
            printf("Perfect Square");
            return 0;
             
         }
    }
    printf("Not a Perfect Square");
}



/*

Output:

Enter the Number : 100
Perfect Square

*/

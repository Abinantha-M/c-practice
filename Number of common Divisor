#include <stdio.h>
int main()
{
int a,b,ref,count=0;
printf("Enter A and B : ");
scanf("%d %d",&a,&b);
if (a<b)
    ref=a;
else
    ref=b;
for(int i= 1;i<=ref;i++)
{
    if((a%i==0) && (b%i==0))
        count++;
}
printf("NO of Common divisiors : %d",count);
}



/*

Output:

Enter A and B : 187 365
NO of Common divisiors : 1

*/
